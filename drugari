CREATE TABLE drugari (
      id INTEGER,
   name TEXT,
   birthday DATE
);
insert into drugari (id,name,birthay) values (1,'sanja',1987-05-31);
select * from friends;
insert into drugari (2,'sale',1988-10-11);
insert into drugari (3,'meta',1987-07-21);
update drugari
set meta = 'marieta'
where id = 3;
alter table drugari
add column email text;
